{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "outDir": "./lib",
    "rootDir": "./src",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.promise"
    ],
    "moduleResolution": "node",
    "skipLibCheck": true
  }
}